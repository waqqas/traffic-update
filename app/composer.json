{
    "name": "roadez/trafficupdate",
    "description": "Traffic Update",
    "keywords": ["yii2", "framework", "traffic", "sms", "roadez"],
    "homepage": "http://www.roadez.com/",
    "type": "project",
    "license": "Proprietary",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "jakeasmith/http_build_url": "^1.0",
        "giggsey/libphonenumber-for-php": "^7.2",
        "hightman/httpclient": "*",
        "flow/jsonpath": "^0.3.1",
        "nategood/httpful": "^0.2.20",
        "omnilight/yii2-scheduling": "^1.0",
        "lajax/yii2-translate-manager": "^1.5",
        "richweber/yii2-google-translate-api": "^1.0",
        "raoul2000/yii2-workflow": "^1.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
